{
  "name": "virtualroom",
  "version": "1.0.0",
  "description": "An RESTful API for the todo application",
  "main": "build/index.js",
  "author": "Juan Manuel <manuel.garcia@ironbit.com.mx>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "yarn lint",
    "prestart:prod": "yarn build",
    "build": "babel src --out-dir build",
    "clean": "rm -rf build",
    "clean:modules": "rm -rf node_modules",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix'",
    "start": "nodemon src/app.ts",
    "start:debug": "ndb nodemon src/app.js",
    "start:prod": "node build/index.js",
    "typeorm": "ts-node -r tsconfig-paths -r ./scripts/loadEnv.js node_modules/typeorm/cli"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.3",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.1.3",
    "@nestjs/platform-express": "^7.1.3",
    "@nestjs/typeorm": "^7.1.0",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "lodash": "^4.17.15",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/lodash": "^4.14.155",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
